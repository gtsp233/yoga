  box-shadow: 0 4px 8px rgba(35,27,34,0.45);
}

.c0:active {
  background-color: rgba(35,27,34,0.75);
  color: #FFFFFF;
}

.c0:active svg {
  fill: #FFFFFF;
}

.c0:disabled {
  background-color: #F5F5FA;
  color: #D7D7E0;
  cursor: not-allowed;
}

.c0:disabled svg {
  fill: #D7D7E0;
}

.c1 {
  background-color: transparent;
  border-color: transparent;
  color: #231B22;
}

.c1 svg {
  fill: #231B22;
}

.c1:not([disabled]):hover,
.c1:not([disabled]):focus,
.c1:not([disabled]):active {
  background-color: transparent;
  box-shadow: none;
}

.c1:not([disabled]):hover {
  color: rgba(35,27,34,0.5);
}

.c1:not([disabled]):hover svg {
  fill: rgba(35,27,34,0.5);
}

.c1:not([disabled]):focus,
.c1:not([disabled]):active {
  color: rgba(35,27,34,0.75);
}

.c1:not([disabled]):focus svg,
.c1:not([disabled]):active svg {
  fill: rgba(35,27,34,0.75);
}

.c1:disabled {
  background-color: transparent;
  border-color: transparent;
  color: #D7D7E0;
}

.c1:disabled svg {
  fill: #D7D7E0;
}

<div>
  <button
    class="c0 c1"
  >
    <svg />
    Button
  </button>
</div>
`;
