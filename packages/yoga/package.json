{
  "name": "@gympass/yoga",
  "version": "7.85.1",
  "description": "Gympass component library",
  "main": "./src",
  "types": "./src",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:types && yarn build:web && yarn build:esm && yarn build:native",
    "build:types": "tsup --outDir dist/typings --dts-only",
    "build:web": "tsup src/**/web !**/*.test.jsx.snap !**/*.test.tsx.snap --outDir dist/cjs --format=cjs",
    "build:esm": "tsup --format=esm --legacy-output",
    "build:native": "tsup src/**/native !**/*.test.jsx.snap !**/*.test.tsx.snap --outDir dist/cjs --format=cjs",
    "prebuild": "rm -rf ./dist",
    "postbuild": "cp -r ./src/Fonts ./dist/fonts",
    "prepublishOnly": "node ../../scripts/prepublish.js --rn"
  },
  "keywords": [
    "Gympass",
    "components",
    "styled-components",
    "react",
    "design-system"
  ],
  "homepage": "https://github.com/Gympass/yoga#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gympass/yoga.git"
  },
  "bugs": {
    "url": "https://github.com/Gympass/yoga/issues"
  },
  "dependencies": {
    "@gympass/yoga-common": "^1.3.0",
    "@gympass/yoga-helpers": "^1.0.4",
    "@gympass/yoga-icons": "^1.17.0",
    "@gympass/yoga-illustrations": "^0.5.2",
    "@gympass/yoga-system": "^0.11.1",
    "@gympass/yoga-tokens": "^3.2.2",
    "@ptomasroos/react-native-multi-slider": "^2.1.1",
    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.0",
    "date-fns": "^2.29.2",
    "deprecated-react-native-prop-types": "^2.3.0",
    "downshift": "^5.0.0",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.7.2",
    "proxy-polyfill": "^0.3.1",
    "rc-slider": "^8.7.1",
    "react-dom": "^18.2.0",
    "react-google-font-loader": "^1.1.0",
    "react-phone-input-2": "^2.15.1"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "react-native": "0.72.3",
    "styled-components": "^4.4.0"
  },
  "peerDependencies": {
    "@react-native-picker/picker": "^2.4.9",
    "react": ">=16",
    "react-native": "0.72.3",
    "styled-components": "^4.4.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/**/*.ts"
    ],
    "splitting": false,
    "bundle": false
  }
}
